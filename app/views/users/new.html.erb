<%= form_for @user do |f|%>
  Username: <%=  f.text_field :username %><br>
  Email: <%= f.text_field :email, :value => "example@mail.com" %><br>
  Password: <%=  f.password_field :password %><br>
  Re Password: <%=  f.password_field :password_confirmation %><br>
  <%= f.submit "Create"%>
<% end %>


